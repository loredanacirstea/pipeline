{
    "extends": [
        "airbnb-base",
        "plugin:vue/essential"
    ],
    "rules": {
        "indent": ["error", 4],
        "object-curly-spacing": ["error", "never"],
        "object-curly-newline": ["error", { "multiline": true }],
        "no-plusplus": "off",
        "func-names": "off",
        "import/prefer-default-export": "off",
        "no-console": "off",
        "arrow-parens": "off",
        "object-shorthand": "off",
        "space-before-function-parens": "off"
    }
}
